<h1>
  Sessions
  <%= user_signed_in? && policy(Session).new? ? link_to('New Session', new_session_path, class: "btn btn-primary") : '' %>
</h1>

<% @sessions.each do |session| %>
  <p>
    <%= cl_image_tag(session.material.key, :height=>100, :width=>150, crop: :limit, cloud_name: 'macreadiebri') if session.material.attached? %>
    <%= link_to session.title, session %> made by <%= link_to session.gamemaster.name, session.gamemaster %>
  </p>
<% end %>
